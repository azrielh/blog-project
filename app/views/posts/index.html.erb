<% @posts.each do |p| %>
  <div class="post_wrapper">
    <h2 class="title"><%= link_to p.title, post_path(p) %></h2>
    <p class="date"><%= p.created_at.strftime("Created on %B, %d, %Y") %></p>
    <p>Created By: <%= p.user.full_name %></p>
  </div>
<% end %>
<!-- kaminari -->
<%= paginate @posts %>
